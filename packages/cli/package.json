{
  "name": "tracerbench",
  "version": "6.1.1",
  "description": "CLI for TracerBench",
  "keywords": [
    "oclif"
  ],
  "repository": "https://github.com/TracerBench/tracerbench/tree/master/packages/cli",
  "homepage": "https://www.tracerbench.com",
  "bugs": "https://github.com/TracerBench/tracerbench/issues",
  "license": "BSD-2-Clause",
  "author": "Kris Selden @krisselden",
  "files": [
    "bin",
    "dist",
    "src",
    "oclif.manifest.json",
    "tb-schema.json"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "rehearsal": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.5",
    "@oclif/parser": "^3.8.5",
    "@oclif/plugin-help": "^3.2.3",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "archiver": "^5.3.0",
    "chalk": "^4.1.1",
    "chrome-debugging-client": "^2.0.0",
    "cli-table3": "^0.6.0",
    "devtools-protocol": "^0.0.936174",
    "execa": "^5.0.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "html-minifier-terser": "^5.1.1",
    "json-query": "^2.2.2",
    "json5": "^2.2.0",
    "listr": "^0.14.3",
    "path": "0.12.7",
    "tmp": "^0.2.1",
    "tough-cookie": "^4.0.0",
    "tslib": "^2.2.0",
    "@rehearsal/plugin-ts-migrate": "^0.0.9",
    "@rehearsal/reporter": "^0.0.9",
    "@rehearsal/tsc-transforms": "^0.0.9",
    "compare-versions": "^4.1.1",
    "debug": "^4.3.2",
    "find-up": "^5.0.0",
    "listr2": "^3.13.4",
    "semver": "^7.3.5",
    "ts-migrate": "^0.1.26",
    "ts-migrate-plugins": "^0.1.26",
    "ts-migrate-server": "^0.1.26",
    "which": "^2.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.17",
    "@types/fs-extra": "^9.0.11",
    "@types/html-minifier-terser": "^5.1.1",
    "@types/json-query": "^2.2.0",
    "@types/json5": "^2.2.0",
    "@types/listr": "^0.14.3",
    "@types/log-symbols": "^3.0.0",
    "@types/mock-fs": "4.13.1",
    "@types/node": "^15.0.2",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "chai": "^4.3.4",
    "chai-files": "^1.4.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-oclif": "^0.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "mocha": "^9.1.3",
    "mock-fs": "^4.14.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "typescript-json-schema": "^0.50.0",
    "@microsoft/api-documenter": "^7.13.68",
    "@microsoft/api-extractor": "^7.18.19",
    "@types/debug": "^4.1.7",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "oclif": {
    "commands": "./dist/src/commands",
    "bin": "rehearsal",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "registry": "https://www.npmjs.com/package/tracerbench"
    },
    "topics": {
      "compare:analyze": {
        "description": "Run an analysis of a benchmark run from a results json file and output to terminal"
      }
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .ts .",
    "prepare": "yarn build",
    "prepack": "yarn build && yarn build:schema",
    "test": "yarn lint && yarn nyc mocha",
    "oclif": "oclif-dev manifest && oclif-dev readme && yarn oclif:markdown",
    "oclif:markdown": "oclif-dev readme --dir ./markdown --multi",
    "build": "yarn tsc -b && yarn oclif && yarn lint",
    "build:schema": "typescript-json-schema \"src/command-config/tb-config.ts\" ITBConfig --out ./tb-schema.json",
    "copystatic": "cp -a ./src/static ./dist/src"
  }
}
